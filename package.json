{
  "name": "install-webpack",
  "version": "1.0.0",
  "description": "webpack创建react项目",
  "main": "index.js",
  "scripts": {
    "dll": "webpack --config ./webpack.config/webpack.dll.js",
    "build": "webpack --env production --config ./webpack.config/index.js",
    "dev": "webpack-dev-server  --env development --config ./webpack.config/index.js",
    "server": "webpack serve  --env production --config ./webpack.config/index.js",
    "initEslint": "eslint --init",
    "initTs": "tsc --init",
    "eslint": "npx eslint .",
    "dist": "node run-dist.js",
    "start": "npm run dll && npm run dev"
  },
  "keywords": [],
  "author": "Lojiaxin",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "add-asset-html-webpack-plugin": "^6.0.0",
    "babel-loader": "^9.1.3",
    "babel-preset-xin": "^1.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.48.0",
    "eslint-config-xin": "^1.0.2",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.5.3",
    "mime": "^3.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "antd": "^5.9.0",
    "axios": "^1.5.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  }
}